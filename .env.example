# Backend Environment Variables (set in Vercel Dashboard)

# Supabase Database URL (PostgreSQL connection string)
SUPABASE_DB_URL=postgresql://postgres.xxx:password@aws-x-region.pooler.supabase.com:6543/postgres

# Supabase Config
SUPABASE_URL=https://xxx.supabase.co
SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_KEY=your_service_key
SUPABASE_JWT_SECRET=your_jwt_secret

# Email Service (Resend)
RESEND_API_KEY=re_xxx
SENDER_EMAIL=FlexCard <contact@yourdomain.com>

# Frontend URL (your Vercel domain)
FRONTEND_URL=https://your-domain.vercel.app

# Frontend Environment Variables
REACT_APP_BACKEND_URL=
REACT_APP_SUPABASE_URL=https://xxx.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your_anon_key
